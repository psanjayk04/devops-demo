pipeline {
    agent any

    stages {
        stage("server Details") {
            steps {
                echo "Server Details"
            }
        }
        stage("CPU Deatails") {
            steps {
                sh "lscpu"
            }
        }
        stage("Memory Deatails") {
            steps {
                sh "free -h"
            }
        }
    }
}
